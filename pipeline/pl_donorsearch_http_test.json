{
	"name": "pl_donorsearch_http_test",
	"properties": {
		"activities": [
			{
				"name": "Copy donorsearch http data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_donorsearch_REST_test",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_donorsearch_sink_REST_test",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"firstName": {
				"type": "string"
			},
			"lastName": {
				"type": "string"
			},
			"homeState": {
				"type": "string"
			}
		},
		"annotations": []
	}
}